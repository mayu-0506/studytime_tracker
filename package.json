{
  "name": "studytime_tracker3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:big": "cross-env NODE_OPTIONS='--max-http-header-size=32768' next dev",
    "dev:big:win": "set NODE_OPTIONS=--max-http-header-size=32768 && next dev",
    "dev:big:unix": "NODE_OPTIONS='--max-http-header-size=32768' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-versions": "node scripts/check-versions.js",
    "health-check": "npm run check-versions && npm run lint && npm run test && npm run build",
    "dev-large-headers": "node --max-http-header-size=16384 node_modules/.bin/next dev",
    "cookie-monitor": "node scripts/monitor-cookies.js",
    "cookie-health-check": "node scripts/cookie-health-check.js",
    "debug-431": "npm run cookie-monitor && npm run cookie-health-check",
    "migrate:images": "tsx scripts/migrate-profile-images.ts",
    "migrate:storage": "tsx scripts/migrate-images-to-storage.ts",
    "admin:cleanup": "tsx scripts/cleanup-large-cookies.ts",
    "db:migrate:sql": "echo 'Run scripts/sql/migrate-base64-images.sql in Supabase SQL Editor'",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:full": "rm -rf .next node_modules package-lock.json && npm install",
    "dev:fresh": "npm run clean && npm run dev",
    "reset:problem-users": "tsx scripts/reset-problem-users.ts",
    "users:check": "tsx scripts/check-auth-users.ts",
    "users:hard-delete": "tsx scripts/hard-delete-users.ts",
    "users:check:one": "tsx scripts/check-specific-user.ts",
    "users:hard-delete:one": "tsx scripts/hard-delete-specific-user.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "base64-arraybuffer": "^1.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "postcss": "8.4.31",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "react-images-uploading": "^3.1.7",
    "sharp": "^0.34.2",
    "swr": "^2.3.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
